    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      ".git",
      "node_modules",
      "*.spec.js",
      "*.test.js",
      "__tests__",
      "dist",
      "admin_ui"
    ]
  }
