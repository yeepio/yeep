{
  "name": "yeep",
  "version": "1.0.0-alpha.1",
  "description": "User management made easy",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --require dotenv/config dist/index.js",
    "dev:server": "nodemon server/index.js --exec 'node --require @babel/register'",
    "build:server": "rollup -c",
    "dev:admin": "webpack-dev-server --config=admin_ui/webpack.config.js --env.development",
    "build:admin": "webpack --progress --colors --config=admin_ui/webpack.config.js --env.production",
    "build": "npm run build:server && npm run build:admin",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:admin\"",
    "lint": "eslint .",
    "clean": "rimraf dist",
    "prebuild": "rimraf ./dist",
    "test": "PORT=5002 jest --config=config/jest.json --runInBand",
    "coverage": "jest --config=config/jest.json --coverage",
    "prettier-fix": "prettier --write --config .prettierrc '**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yeepio/yeep.git"
  },
  "keywords": [
    "user",
    "session",
    "login",
    "role",
    "priviledge",
    "rbac"
  ],
  "author": {
    "name": "Dimitrios C. Michalakos"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yeepio/yeep/issues"
  },
  "homepage": "https://github.com/yeepio/yeep#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
